mixin sPartners(data)
	// start sPartners
	+b.SECTION.sPartners.section#sPartners&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 Партнеры
			+e.row.row
				mixin item(i)
					.col-sm-6.col-md-4.col-lg-3
						+e.img-wrap
							+img(`img/svg/sPartners-img-${i}.svg`)
				- for(i=1; i<=8; i++)
					+item(i)
	// end sPartners